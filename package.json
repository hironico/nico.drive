{
  "name": "nico.drive",
  "version": "7.1.0",
  "description": "WebDav Server and cool user interface for web and mobile.",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "eslint --fix src/**",
    "build": "tsc",
    "postbuild": "cp package.json ./dist/.",
    "prestart": "npm run build",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run start",
